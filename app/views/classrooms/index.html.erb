<div class="admin-block">
  <p id="notice"><%= notice %></p>

  <h1>
    Classrooms
    <%= link_to 'New Classroom', new_classroom_path , class: 'btn-ner'%>
  </h1>

  <table>
    <thead>
      <tr>
        <th>Name</th>
        <th>Spec</th>
        <th>Course</th>
        <th>Teacher</th>
        <th>Seat</th>
        <th>Booked</th>
        <th>Pass</th>
        <th>Status</th>
        <th>Schedule</th>
        <th>Start</th>
        <th>End</th>
        <th>Price</th>
        <th>Start time</th>
        <th>End time</th>
        <th>Duration</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @classrooms.each do |classroom| %>
        <tr>
          <td><%= classroom.name %></td>
          <td><%= classroom.spec %></td>
          <td><%= classroom.course %></td>
          <td><%= classroom.teacher %></td>
          <td><%= classroom.seat %></td>
          <td><%= classroom.booked %></td>
          <td><%= classroom.pass %></td>
          <td><%= classroom.status %></td>
          <td><%= classroom.schedule %></td>
          <td><%= classroom.start %></td>
          <td><%= classroom.end %></td>
          <td><%= classroom.price %></td>
          <td><%= classroom.start_time %></td>
          <td><%= classroom.end_time %></td>
          <td><%= classroom.duration %></td>
          <td><%= link_to 'Show', classroom %></td>
          <td><%= link_to 'Edit', edit_classroom_path(classroom) %></td>
          <td><%= link_to 'Destroy', classroom, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

</div>
